{
    "manifest_version": 3,
    "version": "0.0.5",
    "name": "Obfuscate Adobe",
    "description": "Tool to hide secure data on Adobe services",
    "author": "Jason Ford <jasford@adobe.com",
    "homepage_url": "https://www.adobe.com",
    "icons": {
        "16": "icons/adobe-on-16.png",
        "32": "icons/adobe-on-32.png",
        "48": "icons/adobe-on-48.png",
        "128": "icons/adobe-on-128.png"
    },
    "permissions": ["tabs", "activeTab", "storage", "scripting"],
    "host_permissions": [
        "https://swat.magento.com/*",
        "https://swat.prd.ceng.magento.com/*",
        "https://one.newrelic.com/*"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}
